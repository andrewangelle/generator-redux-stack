{
  "name": "<%= appName %>",
  "version": "0.1.0",
  "description": "<%= appDescription %>",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node server/index.js",
    "clean": "rimraf lib dist",
    "lint": "eslint src test",
    "test": "cross-env NODE_ENV=test jest",
    "test:update": "cross-env NODE_ENV=test jest -u",
    "test:watch": "npm test -- --watchAll",
    "check": "npm run lint && npm run test ",
    "build": "cross-env NODE_ENV=production webpack src/index.js --config webpack.config.production.js",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "jest": {
    "testRegex": "(/test/.*|(\\.|/))\\.js?$",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(scss)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "devEngines": {},
  "engines": {},
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "connect-history-api-fallback": "^1.1.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "^2.7.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.1",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    <% if (requireFileLoader) { %>"file-loader": "^0.8.5",<% } %>
    "html-webpack-plugin": "^2.10.0",
    "isparta": "^4.0.0",
    "jest": "^18.1.0",
    "jest-css-modules": "^1.1.0",
    "jsdom": "^7.2.2",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "react-addons-test-utils": "^15.0.0",
    "react-css-modules": "^3.7.6",
    "react-test-renderer": "^15.4.2",
    "redux-devtools": "^3.1.1",
    "redux-devtools-log-monitor": "^1.0.4",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    <% if (requireApiServer) { %>"webpack-dev-server": "^1.14.1",<% } %>
    "webpack-hot-middleware": "^2.7.1"
  },
  "dependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^1.2.1",
    "redux-logger": "^2.5.2",
    "redux-thunk": "^1.0.3"
  }
}
